{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Simple",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "object",
      "properties": {
        "street": {
          "type": "string"
        }, "number": {
          "type": "integer"
        }
      }
    }
  },
  "required": ["address"]
}